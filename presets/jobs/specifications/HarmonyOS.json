{
  "templates": {
    "kernel": {
      "plugins": [
        {
          "name": "EMG",
          "options": {
            "generators options": [
              {
                "genericFunctions": {
                  "infinite calls sequence": false,
                  "initialize strings as null terminated": false,
                  "process per call": true,
                  "functions to call": ["OsMain", "SysTyme", "ConsoleSendTask", "OsArmA32SyscallHandle", "OsResourceRecoveryTask", "OsSwtmrTask", "SysAccept", "SysAccess", "SysBind", "SysBrk", "SysChdir", "SysChmod", "SysChown", "SysClose", "SysConnect"]
                }
              }
            ],
            "translation options": {
              "entry point": "harmony_main",
              "self parallel processes": false,
              "do not skip signals": true,
              "allocate external": false,
              "environment model file": "environment_model.c",
              "additional headers": [
                "pthread.h",
                "stddef.h",
                "ldv/verifier/common.h",
                "ldv/verifier/nondet.h"
              ]
            }
          }
        },
        {
          "name": "RSG",
          "options": {
            "common sets model": "counter",
            "model compiler input file": "kernel/liteos_a/kernel/common/los_config.c",
            "common models": {
              "verifier/common.c": {},
              "verifier/gcc.c": {},
              "verifier/nondet.c": {},
              "harmonyos/common.c": {}
            }
          }
        },
        {"name": "Weaver"},
        {
          "name": "FVTP",
          "options": {
            "merge source files": true,
            "verifier profile": "reachability",
            "CIL additional opts": [],
            "verifier": {
              "name": "CPAchecker",
              "version": "trunk:31140"
            }
          }
        }
      ]
    },
    "concurrency safety for HarmonyOS": {
      "template": "kernel",
      "plugins": [
        {
          "name": "EMG",
          "options": {
            "check final state": false,
            "translation options": {
              "self parallel processes": false,
              "do not skip signals": true,
              "direct control functions calls": false
            }
          }
        },
        {
          "name": "RSG",
          "options": {"models": ["harmonyos/concurrency safety/synchronization primitives.c"]}
        },
        {"name": "Weaver"},
        {
          "name": "FVTP",
          "options": {
            "verifier profile": "race checking lightweight liteos",
            "verifier": {
              "name": "CPAchecker",
              "version": "CPALockator-combat-mode:34860"
            },
            "result processing": {
              "expect several witnesses": true,
              "code coverage details": "None"
            }
          }
        }
      ]
    }
  },
  "requirement specifications": {
    "description": "Harmony OS requirement specifications",
    "template": "kernel",
    "children": [
      {
        "identifier": "empty",
        "description": "..."
      },
      {
        "identifier": "concurrency safety",
        "description": "Harmony requirement specifications",
        "template": "concurrency safety for HarmonyOS"
      }
    ]
  }
}
